<odoo>
    <record id="view_location_form_inherit" model="ir.ui.view">
        <field name="name">stock.location.form.inherit</field>
        <field name="model">stock.location</field>
        <field name="inherit_id" ref="stock.view_location_form"/>
        <field name="arch" type="xml">
            <field name="name" position="after">
                <label for="code"/>
                <field name="code" string="Location Code"/>
            </field>
        </field>
    </record>

    <record id="view_location_tree_inherit" model="ir.ui.view">
        <field name="name">stock.location.tree.inherit</field>
        <field name="model">stock.location</field>
        <field name="inherit_id" ref="stock.view_location_tree"/>
        <field name="arch" type="xml">
            <field name="complete_name" position="after">
                <field name="code" string="Codigo Netbox"/>
            </field>
        </field>
    </record>

    
</odoo>
